@namespace LumexUI
@inherits LumexComponentBase

<LumexComponent As="@As"
                Class="@RootClass"
                Style="@RootStyle"
                data-disabled="@Utils.GetDataAttributeValue( Disabled )"
                @attributes="@AdditionalAttributes">
    <NavLink Match="@Match"
             ActiveClass="@ActiveClass"
             href="@Href">
        @ChildContent
    </NavLink>
</LumexComponent>
