@namespace LumexUI
@inherits LumexComponentBase

<CascadingValue TValue="NavbarContext" Value="@_context" IsFixed="@true">
    <LumexComponent As="@As"
                    Class="@RootClass"
                    Style="@RootStyle"
                    data-slot="base"
                    @attributes="@AdditionalAttributes">
        <nav class="@WrapperClass" data-slot="wrapper">
            @ChildContent
        </nav>
    </LumexComponent>
</CascadingValue>
